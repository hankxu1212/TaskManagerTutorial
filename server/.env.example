# Server Configuration
PORT=8000
NODE_ENV=development

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/taskmanager?schema=public

# AWS Configuration
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-west-2
S3_BUCKET_NAME=your-s3-bucket-name
STAGE=dev

# Development Account (only used when NODE_ENV=development)
# These values are used to bypass Cognito auth during local development
DEV_FULL_NAME=Your Name
DEV_ACCOUNT_NAME=your_username
DEV_ACCOUNT_EMAIL=your@email.com
DEV_ACCOUNT_ID=1
